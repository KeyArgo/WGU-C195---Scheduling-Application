          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="350.0" VBox.vgrow="ALWAYS">
      <children>
        <Label fx:id="warning" alignment="CENTER" layoutX="3.0" layoutY="271.0" prefHeight="15.0" prefWidth="342.0" style="&#10;" text="This system is for the use of authorized users only." textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
